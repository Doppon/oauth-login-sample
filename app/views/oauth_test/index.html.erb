<h1>OauthTest#index</h1>

 <p><%= notice %><%= alert %></p>
 <% if user_signed_in? %>
   <%= link_to "ログアウト", destroy_user_session_path, method: :delete %>
 <% else %>
   <%= link_to 'githubでサインアップ', user_github_omniauth_authorize_path %>
 <% end %>

<% if user_signed_in? %>
  <p><%= current_user.id %></p>
  <p><%= current_user.name %></p>
  <p><%= current_user.email %></p>
<% end%>
